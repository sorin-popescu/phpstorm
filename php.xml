<application>
  <component name="MessDetector">
    <phpmd_settings_migrated />
    <phpmd_settings>
      <MessDetectorConfiguration tool_path="$USER_HOME$/.composer/vendor/bin/phpmd" />
    </phpmd_settings>
  </component>
  <component name="PhpCodeSniffer">
    <phpcs_settings_migrated />
    <phpcs_settings>
      <PhpCSConfiguration standards="MySource;PEAR;PSR1;PSR2;Squiz;Zend" />
    </phpcs_settings>
  </component>
</application>